citHeader("To cite powRICLPM in publications use:")

citEntry(
  entry    = "Article",
  title    = "Power analysis for the random intercept cross-lagged panel model using the powRICLPM R-package",
  author   = personList(as.person("Jeroen D. Mulder")),
  journal  = "Structural Equation Modeling: A Multidisciplinary Journal",
  year     = "2022",
  url      = "https://doi.org/10.1080/10705511.2022.2122467",
  textVersion =
  paste("Mulder, J.D. (2022).",
        "Power analysis for the random intercept cross-lagged panel model using the powRICLPM R-package.",
        "Structural Equation Modeling: A Multidisciplinary Journal.",
        "https://doi.org/10.1080/10705511.2022.2122467")
)
